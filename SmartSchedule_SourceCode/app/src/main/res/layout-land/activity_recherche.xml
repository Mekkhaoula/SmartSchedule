<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layoutt"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbarStyle="insideInset"
    >

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:id="@+id/kkk"
        android:background="@drawable/fff"
        android:layout_height="548dp"
        android:weightSum="1">


        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_gravity="center_horizontal"
            android:background="#a3000000">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Smart Schedule"
                android:id="@+id/textView15"
                android:layout_gravity="center_horizontal|top"
                android:textSize="30dp"
                android:textColor="#ff00baba" />

            <Button
                android:layout_width="53dp"
                android:layout_height="50dp"
                android:id="@+id/button29"
                android:layout_gravity="left|center_vertical"
                android:background="@drawable/riight"
                android:onClick="backRecherche" />

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="55dp"
                android:background="#00191919"
                android:onClick="addToFav"
                android:alpha="0.85"
                android:layout_gravity="right|center_vertical"
                android:id="@+id/g">

            </FrameLayout>

        </FrameLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#00000000"
            android:weightSum="1"
            android:id="@+id/mm">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:id="@+id/textView64"
                android:layout_gravity="center_horizontal"
                android:background="#ff00baba" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_gravity="center_horizontal"
                android:background="#a3000000">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:weightSum="1"
                    android:background="#00000000"
                    android:layout_gravity="left|center_vertical">

                    <FrameLayout
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="#ff00baba" />

                    <Button
                        android:layout_width="80dp"
                        android:layout_height="60dp"
                        android:text="Annexe 1"
                        android:id="@+id/button13"
                        android:textColor="#ff00baba"
                        android:background="#00ff3053"
                        android:onClick="annex1"
                        android:layout_weight="0.3" />

                    <FrameLayout
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="#ff00baba" />

                    <Button
                        android:layout_width="80dp"
                        android:layout_height="60dp"
                        android:text="Annexe 2"
                        android:id="@+id/button14"
                        android:textColor="#ff00baba"
                        android:background="#00ff3053"
                        android:onClick="annex2" />

                    <FrameLayout
                        android:layout_width="2dp"
                        android:layout_height="match_parent"
                        android:background="#ff00baba" />

                    <Button
                        android:layout_width="84dp"
                        android:layout_height="wrap_content"
                        android:text="Centrale"
                        android:id="@+id/button15"
                        android:textColor="#ff00baba"
                        android:background="#00ff3053"
                        android:onClick="centrale" />

                    <FrameLayout
                        android:layout_width="2dp"
                        android:layout_height="60dp"
                        android:background="#ff00baba" />

                </LinearLayout>

                <FrameLayout
                    android:layout_width="52dp"
                    android:layout_height="55dp"
                    android:background="#00191919"
                    android:onClick="addToFav"
                    android:alpha="0.85"
                    android:layout_gravity="right|center_vertical">

                    <ImageView
                        android:layout_width="65dp"
                        android:layout_height="65dp"
                        android:id="@+id/imageView12"
                        android:background="@drawable/tr"
                        android:layout_gravity="left|center_vertical" />

                </FrameLayout>
            </FrameLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:id="@+id/textView74"
                android:background="#ff00baba"
                android:layout_gravity="center_horizontal" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:weightSum="1">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="89dp"
                    android:layout_height="match_parent"
                    android:background="#000024ff">

                    <ListView
                        android:layout_width="93dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/saale2"
                        android:divider="#00baba"
                        android:listSelector="#00baba"
                        android:background="#a3000000" />
                </LinearLayout>

                <TextView
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:id="@+id/textView75"
                    android:background="#ff00baba" />

                <WebView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/webView2" />
            </LinearLayout>

        </LinearLayout>


    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="250dp"

        android:layout_weight="0.18"
        android:alpha="0.85"
        android:id="@+id/gg"
        android:background="#00baba"
        android:layout_gravity="left|start">
        <CalendarView
            android:id="@+id/calendar_view"
            android:layout_width="250dp"
            android:layout_height="match_parent"
            android:weekNumberColor="#ff0d0e0e"
            android:background="#ff00baba"
            android:weekSeparatorLineColor="#ff030101" />

    </LinearLayout>


</android.support.v4.widget.DrawerLayout>
